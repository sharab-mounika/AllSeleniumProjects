package TestNGPackage;

import static org.testng.Assert.assertEquals;

import org.testng.Assert;
import org.testng.AssertJUnit;
import org.testng.annotations.Test;

public class AssertionsDemo {
	
	@Test
	public void test1() {
		System.out.println("open browser ");
		Assert.assertEquals(true, false);			// hard assert
		// Hard assertion - if any assertion gets failed, immediately lines after assertion will not be executed and test case will be marked as failed 
		System.out.println("line1 after hard assert");
		System.out.println("line2 after hard assert");
	}

}
